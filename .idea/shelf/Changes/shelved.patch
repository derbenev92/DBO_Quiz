+        this.id = id;
+        this.correct = correct;
+        this.incorrect = incorrect;
+        this.login = login;
+        this.level = level;
+    }
+
+
+
+
+    public String getId() {
+        return id;
+    }
+
+    public void setId(String id) {
+        this.id = id;
+    }
+
